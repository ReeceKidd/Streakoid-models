    "license": "ISC",
    "devDependencies": {
        "@types/dotenv": "^6.1.1",
        "@types/jest": "^24.0.18",
        "@types/mongoose": "^5.5.17",
        "eslint": "^6.5.1",
        "husky": "^3.0.5",
        "lint-staged": "^10.0.0-0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "tslint": "^5.20.0",
        "typescript": "^3.6.3"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^2.3.2",
        "@typescript-eslint/parser": "^2.3.2",
        "axios": "^0.19.0",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-prettier": "^3.1.1"
    },
    "directories": {
        "lib": "lib",
        "test": "tests"
    },
